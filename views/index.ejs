    <div class="container">
      <div class="hero-unit">
        <h1>
          Webshot
          <small>from <a href="http://okfnlabs.org/">Open Knowledge Foundation Labs</a></small>
          <br />
          <small>Free, automated generation of live screenshots of public websites</small>
        </h1>
      </div>
      <div>
        <div class="row">
          <div class="span6">
            <h2>Try it out</h2>
            <form action="/api/generate" class="form js-generate" style="margin-top: 30px;">
              <div class="control-group">
                <label for="url" class="control-label">URL</label>
                <div class="controls">
                  <input type="text" name="url" id="url"
                    class="input-xlarge"
                    placeholder="http://okfn.org/"
                    type="url"
                    required=""
                    />
                </div>
              </div>
                <fieldset>
                    <legend>Optional parameters</legend>
                    <label for="width">Width</label>
                    <input type="number" name="width" id="width" class="input-small" value="1024" min="100" max="2560" />

                    <label for="height">Height</label>
                    <input type="number" name="height" id="height" class="input-small" value="768" min="100" max="" />

                    <label for="delay">Delay (in ms)</label>
                    <input type="number" name="delay" id="delay" class="input-small" value="0" min="0" max="10000" step="100" />

                    <label for="userAgent">User agent</label>
                    <input type="text" name="userAgent" id="userAgent" class="input-xlarge" value=""  />

                    <label for="full">
                      <input type="checkbox" name="full" id="full" />
                      Grab whole page (not just visible page)
                    </label>
                </fieldset>
                <div class="control-group" style="margin-top: 30px;">
                  <div class="controls">
                    <button type="submit" class="btn btn-primary btn-large js-generate-btn">Generate screenshot</button>
                  </div>
                </div>
            </form>
          </div>
          <div class="span6">
            <h2>API</h2>
            <dl>
              <dt>HTTP Method</dt>
              <dd><code>GET</code></dd>
              <dt>URL</dt>
              <dd><code>/api/generate</code></dd>
              <dt>Parameters</dt>
              <dd>
                <dl>
                  <dt><code>url</code></dt>
                  <dd>Required, must include the protocol</dd>
                  <dt><code>width</code></dt>
                  <dd>Not required, defaults to 1024</dd>
                  <dt><code>height</code></dt>
                  <dd>Not required, defaults to 768</dd>
                  <dt><code>delay</code></dt>
                  <dd>Not required, specified in ms, defaults to 0ms</dd>
                  <dt><code>userAgent</code></dt>
                  <dd>Not required, specifies the user agent</dd>
                  <dt><code>full</code></dt>
                  <dd>Not required, specifies whether the full page should be captured, only true/false is allowed, defaults to false</dd>
                </dl>
              </dd>
            </dl>
          </div>
        </div>
      </div>
     
    <div id="results" style="display:none;">
      <h2>Screenshot</h2>
      <p>Image Link: <a href="" id="url"></a></p>
      <img id="webshot" />
    </div>
</div>  

